{
  "version": "1.1.0",
  "component_kind": null,
  "queries": {
    "$my_feed": {
      "authors": [
        "user.pubkey"
      ],
      "limit": 20
    }
  },
  "elements": [
    {
      "type": "loop",
      "source": "$my_feed",
      "variable": "$note",
      "elements": [
        {
          "type": "span",
          "content": [
            "{$note.content}"
          ]
        }
      ]
    }
  ]
}