{
  "version": "1.1.0",
  "component_kind": null,
  "queries": {
    "$my_feed": {
      "kinds": [
        1
      ],
      "authors": [
        "user.pubkey"
      ],
      "limit": 20,
      "pipe": [
        {
          "operation": "reverse"
        }
      ]
    }
  },
  "events": {
    "@post_message": {
      "kind": 1,
      "content": "{form.message}",
      "tags": [
        [
          "client",
          "hypernote-test"
        ]
      ]
    }
  },
  "elements": [
    {
      "type": "h1",
      "content": [
        "My Feed (Oldest First)"
      ]
    },
    {
      "type": "form",
      "event": "@post_message",
      "elements": [
        {
          "type": "input",
          "content": [],
          "attributes": {
            "name": "message",
            "placeholder": "Enter message..."
          }
        },
        {
          "type": "button",
          "elements": [
            {
              "type": "p",
              "content": [
                "Post"
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "loop",
      "source": "$my_feed",
      "variable": "$note",
      "elements": [
        {
          "type": "p",
          "content": [
            "{$note.content}"
          ]
        },
        {
          "type": "json",
          "content": [],
          "attributes": {
            "variable": "$note"
          }
        },
        {
          "type": "json",
          "content": [],
          "attributes": {
            "variable": "$note.content"
          }
        },
        {
          "type": "json",
          "content": [],
          "attributes": {
            "variable": "$note.pubkey"
          }
        }
      ]
    }
  ]
}
