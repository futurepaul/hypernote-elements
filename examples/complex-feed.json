{
  "version": "1.1.0",
  "component_kind": null,
  "styles": {
    "h1": {
      "color": "#1f2937",
      "font-size": 24,
      "font-weight": "bold"
    },
    "button": {
      "padding-top": 12,
      "padding-right": 12,
      "padding-bottom": 12,
      "padding-left": 12,
      "border": {
        "width": 1,
        "style": "solid",
        "color": "#2563eb",
        "radius": 8
      },
      "color": "#ffffff",
      "background-color": "#3b82f6"
    },
    ".card": {
      "padding-top": 16,
      "padding-right": 16,
      "padding-bottom": 16,
      "padding-left": 16,
      "border": {
        "width": 1,
        "style": "solid",
        "color": "#e5e7eb",
        "radius": 12
      },
      "background-color": "#ffffff",
      "elevation": 2
    },
    "#header-title": {
      "color": "#3b82f6",
      "text-align": "center"
    }
  },
  "queries": {
    "$following_feed": {}
  },
  "events": {
    "@post_comment": {
      "kind": 1,
      "content": "{form.message}",
      "tags": [
        [
          "e",
          "{target.id}"
        ],
        [
          "p",
          "{target.pubkey}"
        ]
      ]
    }
  },
  "elements": [
    {
      "type": "h1",
      "id": "header-title",
      "content": [
        "Following Feed"
      ]
    },
    {
      "type": "loop",
      "source": "$following_feed",
      "variable": "$note",
      "elements": [
        {
          "type": "p",
          "content": [
            "  ",
            "  ",
            "    "
          ]
        }
      ]
    },
    {
      "type": "h2",
      "content": [
        "{$note.pubkey}"
      ]
    },
    {
      "type": "span",
      "content": [
        "{$note.content}"
      ]
    },
    {
      "type": "p",
      "content": [
        "    ",
        "    "
      ]
    },
    {
      "type": "form",
      "event": "@post_comment",
      "elements": []
    },
    {
      "type": "h1",
      "content": [
        "note-card\"]"
      ]
    },
    {
      "type": "p",
      "content": [
        "      ",
        "      "
      ]
    }
  ]
}